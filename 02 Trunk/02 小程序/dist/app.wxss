  background: rgba(0, 0, 0, 0.7);
  color: #fff;
  font-size: 35rpx;
  line-height: 1.5em;
  margin: 0 auto;
  box-sizing: border-box;
  padding: 20rpx 36rpx;
  text-align: center;
  border-radius: 8rpx;
  z-index: 100;
}
.zan-toast--notitle {
  padding: 36rpx;
}
.zan-toast__icon {
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin: 0 auto;
  padding: 24rpx 30rpx;
  font-size: 76rpx;
  text-align: center;
}
.zan-toast__icon-loading {
  line-height: 0;
}
.zan-toast__icon-loading .zan-loading {
  width: 80rpx;
  height: 80rpx;
}
.zan-toast__icon-image {
  background-size: 80rpx;
  background-position: center;
  background-repeat: no-repeat;
}
.zan-toptips {
  display: block;
  position: fixed;
  -webkit-transform: translateZ(0) translateY(-100%);
  width: 100%;
  min-height: 64rpx;
  top: 0;
  line-height: 2.3;
  font-size: 35rpx;
  text-align: center;
  color: #fff;
  background-color: #e64340;
  z-index: 110;
  opacity: 0;
  transition: all .4s ease;
}
.zan-toptips--show {
  -webkit-transform: translateZ(0) translateY(0);
  opacity: 1;
}
page {
  min-height: 100%;
  background-color: #eee;
}
view {
  box-sizing: border-box;
}
.align-right {
  text-align: right;
}
.align-center {
  text-align: center;
}
.full-width {
  width: 100%;
}
.inline-block {
  display: inline-block;
}
.lr-mg-5 {
  margin-left: 10rpx;
  margin-right: 10rpx;
}
.lr-mg-10 {
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.fr {
  float: right;
}
.fl {
  float: left;
}
.clearfix::before,
.clearfix::after {
  content: '';
  display: block;
  clear: both;
}

/* 行内元素两边对其 */
.keith {
  text-align: justify;
}
.keith::after {
  display: inline-block;
  width: 100%;
  content: '';
}

